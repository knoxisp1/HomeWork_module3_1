calls = 0


def count_calls():
    global calls
    calls += 1


def string_info(string):
    count_calls()
    tek = str(string)
    result = (len(tek)), tek.upper(), tek.lower()
    return result


def is_contains(string, list_to_search):
    count_calls()
    string = str(string).upper()
    list_to_search = list(list_to_search)
    for i in range(len(list_to_search)):
        if str(list_to_search[i]).upper() == string:
            result = True
            break
        else:
            result = False
            continue
    return result


print(string_info('safqf321r452wedfs'))
print(string_info('qwreewtrgesdgf etryhjfgdasdfgrweth'))
print(is_contains('Urban', ['ban', 'BaNaN', 'urBAN']))
print(is_contains('cycle', ['recycling', 'cyclic']))
print(calls)
